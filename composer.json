{
    "name": "drupal/hellocoop",
    "type": "drupal-module",
    "description": "Hellō Identity Provider, enables users to register and log in using their choice of popular social logins, email, or phone—without tracking them.",
    "keywords": ["drupal", "identity provider", "social login"],
    "homepage": "https://drupal.org/project/hellocoop",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Unnikrishnan Bhargava Kurup",
            "homepage": "https://www.drupal.org/u/unnikrishnan",
            "role": "Maintainer"
        }
    ],
    "support": {
        "issues": "https://www.drupal.org/project/issues/hellocoop",
        "source": "https://git.drupalcode.org/project/hellocoop"
    },
    "require": {
        "php": ">=8.1.0",
        "unnikrishnanbhargavakurup/hellocoop": "^0.0.3",
        "drupal/externalauth": "*",
        "composer/installers": "^2.3"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "tbachert/spi": true,
            "phpstan/extension-installer": true,
            "composer/installers": true
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\hellocoop\\": "src/",
            "Drupal\\Tests\\hellocoop\\": "tests/src"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.11",
        "drupal/core-dev": "^11.1",
        "phpunit/php-code-coverage": "^10.1",
        "drupal/coder": "^8.3",
        "phpunit/phpunit": "^10.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "extra": {
        "drupal": {
            "tests": {
                "unit": "tests/src/Unit"
            }
        }
    }
}
